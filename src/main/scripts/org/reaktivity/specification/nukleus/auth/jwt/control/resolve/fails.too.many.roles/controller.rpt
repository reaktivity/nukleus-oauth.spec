property nukleiV05C ${nuklei:directory("target/nukleus-itests").controlCapacity(4096, 4096)}

property controlV05C ${nukleiV05C.control("auth-jwt")}

connect "agrona://stream/bidirectional"
        option agrona:reader ${agrona:broadcastReceiver(controlV05C.controller)}
        option agrona:writer ${agrona:manyToOneWriter(controlV05C.nukleus)}

connected

write 0x00000011
write ${controlV05C.nextCorrelationId()}
write [0x05] "realm"
write 341
write [0x05] "role1"
write [0x05] "role2"
write [0x05] "role3"
write [0x05] "role4"
write [0x05] "role5"
write [0x05] "role6"
write [0x05] "role7"
write [0x05] "role8"
write [0x05] "role9"
write [0x06] "role10"
write [0x06] "role11"
write [0x06] "role12"
write [0x06] "role13"
write [0x06] "role14"
write [0x06] "role15"
write [0x06] "role16"
write [0x06] "role17"
write [0x06] "role18"
write [0x06] "role19"
write [0x06] "role20"
write [0x06] "role21"
write [0x06] "role22"
write [0x06] "role23"
write [0x06] "role24"
write [0x06] "role25"
write [0x06] "role26"
write [0x06] "role27"
write [0x06] "role28"
write [0x06] "role29"
write [0x06] "role30"
write [0x06] "role31"
write [0x06] "role32"
write [0x06] "role33"
