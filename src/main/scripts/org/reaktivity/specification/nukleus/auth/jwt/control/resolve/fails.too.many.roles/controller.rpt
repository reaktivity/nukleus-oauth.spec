write [0x06] "role45"
write [0x06] "role46"
write [0x06] "role47"
write [0x06] "role48"
write [0x0d] "role49TooMany"

write flush

read 0x40000000
read ${controlV05C.correlationId()}

read notify RESOLVED

close
closed
