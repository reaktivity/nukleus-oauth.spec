property nukleiV05N ${nuklei:directory("target/nukleus-itests").controlCapacity(4096, 4096)}

property controlV05N ${nukleiV05N.controlNew()}

connect "agrona://stream/bidirectional"
        option agrona:reader ${agrona:manyToOneReader(controlV05N.nukleus)}
        option agrona:writer ${agrona:broadcastTransmitter(controlV05N.controller)}

connected

read 0x00000011
read (long:correlationIdV05N)
read ${core:string("oauth")}
read [0x05] "RS256"
read 345 49
read [0x05] "role1"
read [0x05] "role2"
read [0x05] "role3"
read [0x05] "role4"
read [0x05] "role5"
read [0x05] "role6"
read [0x05] "role7"
read [0x05] "role8"
read [0x05] "role9"
read [0x06] "role10"
read [0x06] "role11"
read [0x06] "role12"
read [0x06] "role13"
read [0x06] "role14"
read [0x06] "role15"
read [0x06] "role16"
read [0x06] "role17"
read [0x06] "role18"
read [0x06] "role19"
read [0x06] "role20"
read [0x06] "role21"
read [0x06] "role22"
read [0x06] "role23"
read [0x06] "role24"
read [0x06] "role25"
read [0x06] "role26"
read [0x06] "role27"
read [0x06] "role28"
read [0x06] "role29"
read [0x06] "role30"
read [0x06] "role31"
read [0x06] "role32"
read [0x06] "role33"
read [0x06] "role34"
read [0x06] "role35"
read [0x06] "role36"
read [0x06] "role37"
read [0x06] "role38"
read [0x06] "role39"
